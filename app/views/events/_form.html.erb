<%= simple_form_for event do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :people_going %>
  <%= f.input :people_needed %>
  <%= f.input :age_range %>
  <%= f.input :time, as: :date, html5: true %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, class:"button create-button" %>
<% end %>
